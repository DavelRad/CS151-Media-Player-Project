<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="parentVBox" alignment="CENTER" styleClass="file_select_container" stylesheets="@FileSelect.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.sjsu.cs.mediaplayer.FileSelectController">
   <children>
      <Button fx:id="mediaFileButton" mnemonicParsing="false" prefWidth="150.0" style="-fx-cursor: hand;" styleClass="file_select_container" text="Choose media file">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin></Button>
      <Label fx:id="mediaFileLabel" styleClass="file_select_container" text="Label">
         <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </VBox.margin></Label>
      <Button fx:id="subtitleFileButton" mnemonicParsing="false" prefWidth="150.0" style="-fx-cursor: hand;" text="Choose subtitle file" />
      <Label fx:id="subtitleFileLabel" text="Label" />
      <Button fx:id="watchVideoButton" mnemonicParsing="false" prefWidth="150.0" style="-fx-cursor: hand;" text="Watch video!">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin></Button>
      <Label fx:id="subtitleCheckLabel1" layoutX="310.0" layoutY="213.0" />
   </children>
</VBox>
